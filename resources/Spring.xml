<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:p="http://www.springframework.org/schema/p"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">


    <bean class="org.example.SpStudents" name="student1">
        <property name="id" value = "111" />
        <property name="name" value = "Vaibhav" />
        <property name="address" value = "Nagpur" />
    </bean>

    <bean class="org.example.SpStudents" name="student2">
        <property name="id" value = "142" />
        <property name="name" value = "Prafful" />
        <property name="address" value = "VimanNagar" />
    </bean>

<!--    by using p schema-->
    <bean class="org.example.SpStudents" name="student3" p:id="100" p:name="Ramesh" p:address="Warje"/>


</beans>